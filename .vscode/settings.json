{
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "git.enableSmartCommit": true,
  "files.exclude": {
    "**/node_modules/": true,
    "package-lock.json": true,
    "yarn.lock": true,
    "**/.next": true,
    "**/dist/": true
  },
  "git.autofetch": true,
  "editor.fontLigatures": true,
  "editor.emptySelectionClipboard": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showTabs": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "Dhayanandhan",
    "Monorepo",
    "Polyfill",
    "Segoe",
    "Signup",
    "Truthy",
    "Unmount",
    "Workspaces",
    "antialiased",
    "bconnect",
    "browserslist",
    "bwlines",
    "captcha",
    "cellpadding",
    "cellspacing",
    "codegen",
    "copyfiles",
    "dhayaec",
    "docgen",
    "ecmascript",
    "fastify",
    "flushall",
    "formik",
    "gitignore",
    "gmail",
    "graphiql",
    "gshop",
    "lerna",
    "lintstaged",
    "lpush",
    "lrange",
    "lspace",
    "mariadb",
    "middlewares",
    "minmax",
    "nbsp",
    "nohoist",
    "ormconfig",
    "precommit",
    "prefetch",
    "preheader",
    "proviced",
    "pursuitapp",
    "reakit",
    "rebass",
    "repo",
    "resave",
    "rspace",
    "slugify",
    "tsutils",
    "typeorm",
    "unfetch",
    "varchar",
    "wcss",
    "woff",
    "zeit"
  ]
}
